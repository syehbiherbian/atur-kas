# Kas Juwita Environment Variables
# Copy this file to .env and fill in your values

# Database (Neon PostgreSQL)
NUXT_DATABASE_URL=postgresql://username:password@host/database?sslmode=require

# Authentication
NUXT_AUTH_SECRET=your-super-secret-key-change-in-production

# WhatsApp Gateway (optional - can also be configured per Kas)
NUXT_WA_GATEWAY_URL=https://api.fonnte.com/send
NUXT_WA_API_KEY=your-wa-gateway-api-key

# Public URL
NUXT_PUBLIC_APP_URL=http://localhost:3000
